      <mapping ext="rst" type="Markdown" />
      <mapping pattern=".editorconfig" type="PLAIN_TEXT" />
      <mapping ext="zsh" type="PLAIN_TEXT" />
      <removed_mapping ext="rst" approved="true" type="ReST" />
    </extensionMap>
  </component>
</application>